{
  "name": "@aboutbits/eslint-config",
  "version": "1.1.1",
  "description": "AboutBits' ESLint config presets",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "readme.md",
    "configs/*"
  ],
  "exports": {
    ".": "./configs/index.js",
    "./react": "./configs/react.js",
    "./next": "./configs/next.js"
  },
  "engines": {
    "npm": ">=8",
    "node": ">=16"
  },
  "scripts": {
    "build": "",
    "checks": "",
    "prepublishOnly": "npm run checks",
    "prepare": "npm run build",
    "preversion": "npm run checks",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aboutbits/eslint-config"
  },
  "keywords": [
    "eslint-config",
    "eslint",
    "typescript",
    "react"
  ],
  "author": "AboutBits",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aboutbits/eslint-config/issues"
  },
  "homepage": "https://github.com/aboutbits/eslint-config",
  "prettier": "@aboutbits/prettier-config",
  "dependencies": {
    "@aboutbits/prettier-config": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0"
  },
  "peerDependencies": {
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "next": ">=12.0.0"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    }
  }
}
